{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true
      },
    "plugins": [
        "react-hooks"
    ],
    "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
    ],
    "import/no-extraneous-dependencies": 0,
    "class-methods-use-this": 0,
    "linebreak-style": "off",
    "arrow-parens": ["error", "as-needed", {
      "requireForBlockBody": true
    }]
}